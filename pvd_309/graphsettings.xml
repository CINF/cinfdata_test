	<field>id</field>
	<name>Id</name>
      </field0>
      <field1>
	<field>type</field>
	<name>Type</name>
      </field1>
      <field2>
	<field>time</field>
	<name>Recorded at</name>
      </field2>
      <field3>
	<field>comment</field>
	<name>Comment</name>
      </field3>
    </mandatory_export_fields>
    <matplotlib_settings>
      <width>900</width>
      <height>600</height>
      <title_size>24</title_size>
